<%= render "layouts/errors", :object => @sentence %>

<% image_url = @rss.entries.first.medias.first.content_url %>
<%= image_tag image_url %>

<%= form_for [@story, @sentence] do |f| %>
  <%= f.hidden_field :image_url, :value => image_url %>
  <%= f.label :user_name %>
  <%= f.text_field :user_name %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.submit %>
<% end %>
